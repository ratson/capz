    "screenshot",
    "screenshots",
    "puppeteer",
    "web",
    "device",
    "chromeless",
    "page",
    "website",
    "site",
    "web",
    "url",
    "resolution",
    "size",
    "screengrab",
    "screen",
    "snapshot",
    "shot",
    "responsive",
    "image",
    "svg",
    "render",
    "html",
    "headless",
    "capture",
    "pic",
    "picture",
    "png",
    "jpg",
    "jpeg"
  ]
}
